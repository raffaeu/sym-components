{
  "name": "sym-components",
  "version": "0.1.5",
  "description": "A collection of Vue.js components, implementing Material Design guidelines",
  "main": "dist/index.js",
  "jsnext:main": "src/index.js",
  "typings": "typings/index.d.ts",
  "directories": {
    "doc": "docs"

  },
  "scripts": {
    "start": "webpack-dev-server --config docs/webpack.config.js",
    "demo": "NODE_ENV=production webpack --config docs/webpack.config.js",
    "build": "del dist && webpack --config build/webpack.config.js",
    "lint": "eslint --ext .js,.vue --fix src __tests__; exit 0",
    "prepublish": "npm run build",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/raffaeu/sym-components.git"
  },
  "keywords": [
    "material",
    "design",
    "SymComponents",
    "sym-components"
  ],
  "author": "Raffaele Garofalo (Raffaeu)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/raffaeu/sym-components/issues"
  },
  "homepage": "https://github.com/raffaeu/sym-components#readme",
  "peerDependencies": {
    "vue": "^2.5.2"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "css-loader": "^0.28.1",
    "eslint": "^3.7.1",
    "eslint-config-spatie": "^1.1.0",
    "jest": "^19.0.0",
    "jest-vue-preprocessor": "^0.2.0",
    "vue": "^2.5.2",
    "vue-loader": "^13.3.0",
    "vue-template-compiler": "^2.5.2",
    "webpack": "^3.6.0",
    "webpack-merge": "^4.1.0"
  },
  "jest": {
    "testRegex": "test.js$",
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/jest-vue-preprocessor"
    }
  }  
}
